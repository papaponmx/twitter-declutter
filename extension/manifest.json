{
  "manifest_version": 2,

  "name": "Twitter de-clutter",
  "description": "This extension removes clutter from Twitter timeline",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://twitter.com/"
  ]
}
